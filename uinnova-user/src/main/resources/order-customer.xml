<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
             http://www.springframework.org/schema/beans/spring-beans.xsd
                   http://code.alibabatech.com/schema/dubbo
                   http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="order-user" owner="xuyy"/>

    <!-- 使用multicast广播注册中心暴露服务地址 -->
    <dubbo:registry id="zkOne" address="zookeeper://140.143.236.197:2181"/>
    <!--不使用注册中心-->
    <!--<dubbo:registry address="N/A"/>-->

    <!--生成一个远程服务的调用代理  protocol支持多协议  version 支持多版本 -->
    <dubbo:reference id="orderService"
                     interface="com.uinnova.order.IOrderService"
                     check="false" protocol="hessian" version="0.1.0"/>

    <dubbo:reference id="orderService2"
                     interface="com.uinnova.order.IOrderService"
                     check="false" protocol="hessian" version="0.2.0"/>


    <dubbo:reference id="orderQueryService"
                     interface="com.uinnova.order.IOrderQueryService"
                     check="false" protocol="dubbo" version="0.1.0"/>


</beans>